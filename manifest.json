{
  "manifest_version": 2,

  "name": "a11y-outline",
  "version": "1.0",

  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": [
      "outline.js"
    ],
    "run_at": "document_start",
    "all_frames": true
  }],

  "icons": {
    "128": "icon-128.png"
  },

  "permissions": [
    "<all_urls>"
  ],

  "web_accessible_resources": [
    "outline.css"
  ]
}
